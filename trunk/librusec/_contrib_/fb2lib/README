def	main(argv):
	z = zipfile.ZipFile(argv[0], 'r')
	for i, fn in enumerate(z.namelist()):
		#print >> sys.stderr, i
		f = z.open(fn)
		#m = hashlib.md5()
		#m.update(f.read())
		#print i, fn, m.hexdigest()
		parser = xml.parsers.expat.ParserCreate()
		parser.StartElementHandler = start_element
		parser.Parse(f.read(), True)
	files += (i + 1)

